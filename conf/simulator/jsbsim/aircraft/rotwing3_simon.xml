  <axis name="LIFT">
    <function name="aero/force/Lift_alpha">
        <description>Lift due to alpha</description>
        <switch>
        <case>
                <condition>(aero/alpha-deg >= -17) and (aero/alpha-deg <= 17)</condition>
                <product>
                        <sum>
                                <product>
                                        <function name="aero/fz"/>
                                        <cos><property>aero/alpha-rad</property></cos>
                                </product>
                                <product>
                                        <function name="aero/fx"/>
                                        <sin><property>aero/alpha-rad</property></sin>
                                </product>
                        </sum>
                        <sum>
                                <value>0.45536</value>
                                <product>
                                        <value>0.5446354</value>
                                        <property>fcs/sin2skew</property>
                                </product>
                        </sum>
                </product>
        </case>
        <case>
                <condition>(aero/alpha-deg < -17) or (aero/alpha-deg > 17)</condition>
                <product>
                        <function name="aero/L_flatplate"/>
                        <sum>
                                <value>0.45536</value>
                                <product>
                                        <value>0.5446354</value>
                                        <property>fcs/sin2skew</property>
                                </product>
                        </sum>
                </product>
        </case>
      </switch>
    </function>
  </axis>


