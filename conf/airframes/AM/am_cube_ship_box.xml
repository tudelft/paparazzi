    <define name="USE_AIRSPEED" value="TRUE"/><!-- Use the sensor, not stating use it in flight -->

    <!-- <module name="guidance" type="indi"/> -->

    <module name="nav_hybrid">
      <define name="NAV_HYBRID_POS_GAIN" value="1"/>
      <define name="NAV_HYBRID_LINE_GAIN" value="1"/>
      <define name="NAV_HYBRID_MAX_AIRSPEED" value="15"/>
      <define name="NAV_HYBRID_SPEED_MARGIN" value="10"/>
      <define name="NAV_HYBRID_NAV_LINE_DIST" value="50"/>
      <define name="NAV_HYBRID_MAX_DECELERATION" value="WP_CONTROL_MAX_DECEL_WP_APPROACH"/>
    </module>

    <module name="ahrs" type="float_cmpl_quat">
      <define name="AHRS_GPS_SPEED_IN_NEGATIVE_Z_DIRECTION" value="FALSE"/>
      <define name="AHRS_HEADING_UPDATE_GPS_MIN_SPEED" value="1.0"/>
      <define name="AHRS_GRAVITY_HEURISTIC_FACTOR" value="0.0"/>
      <define name="AHRS_FC_MAG_ID" value="MAG_RM3100_SENDER_ID"/>
