    <!-- Define axis in hover frame -->
    <define name="BODY_TO_IMU_PHI" value="90.0" unit="deg"/>
    <define name="BODY_TO_IMU_THETA" value="0" unit="deg"/>
    <define name="BODY_TO_IMU_PSI" value="0" unit="deg"/>
