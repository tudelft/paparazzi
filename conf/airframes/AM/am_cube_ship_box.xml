    <define name="MODE_AUTO1" value="AP_MODE_HOVER_DIRECT"/>
    <define name="MODE_AUTO2" value="AP_MODE_NAV"/>
    <define name="MODE_STARTUP" value="AP_MODE_NAV"/>
  </section>

