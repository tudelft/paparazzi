    <define name="ELEVON_ROLL_FACTOR" value="0.7f"/>
    <define name="ELEVON_PITCH_FACTOR" value="-0.6f"/>  <!-- TODO Check sign -->
    <define name="ELEVON_YAW_FACTOR" value="0.9f"/>
