<!DOCTYPE module SYSTEM "module.dtd">

<module name="nn_divergence_landing">
  <doc>
    <description>Neural network landing</description>
  </doc>

  <settings>
    <dl_settings>
      <dl_settings NAME="NN Landing">
        <dl_setting var="thrust_effectiveness" min="0" step="0.05" max="2"/>
        <dl_setting var="nn_thrust_p_gain" min="0" step="0.05" max="2"/>
        <dl_setting var="nn_thrust_i_gain" min="0" step="0.05" max="2"/>
      </dl_settings>
    </dl_settings>
  </settings>
  <header>
    <file name="nn.h"/>
  </header>
  <init fun="nn_init()"/>
  <periodic fun="nn_periodic()" freq="2" autorun="FALSE"/>
  <event fun="nn_cntrl()"/>
  <makefile target="ap|nps">
    <file name="nn.c"/>
  </makefile>
</module>
