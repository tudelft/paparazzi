<!DOCTYPE module SYSTEM "module.dtd">

<module name="Overactuated_mixing" dir="Overactuated_vehicle">
  <doc>
    <description>Overactuated Vehicle Mixing</description>
  </doc>

  <settings>
    <dl_settings>
      <dl_settings NAME="Overactuated_settings">
        <dl_setting var="wind_speed"  min="0" step="1" max="20" shortname="wind_speed" />
        <dl_setting var="activate_longitudinal_over" min="0" step="1" max="1" shortname="long_active" values="FALSE|TRUE" />
        <dl_setting var="activate_lateral_over" min="0" step="1" max="1" shortname="lat_active" values="FALSE|TRUE" />
        <dl_setting var="activate_yaw_over" min="0" step="1" max="1" shortname="yaw_active_overactuated" values="FALSE|TRUE" />
        <dl_setting var="manual_yaw_overactuated" min="0" step="1" max="1" shortname="man_yaw_ctl_overactuated" values="FALSE|TRUE" />

        <dl_setting var="manual_roll_setpoint" min="0" step="1" max="1" shortname="man_roll_set" values="FALSE|TRUE" />
        <dl_setting var="manual_roll_overactuated_module"  min="-9600" step="960" max="9600" shortname="roll_setpoint" />

        <dl_setting var="manual_pitch_setpoint" min="0" step="1" max="1" shortname="man_pitch_set" values="FALSE|TRUE" />
        <dl_setting var="manual_pitch_overactuated_module"  min="-9600" step="960" max="9600" shortname="pitch_setpoint" />

        <dl_setting var="P_az_gain" min="0" step=".1" max="15" shortname="P_lat" />
        <dl_setting var="I_az_gain" min="0" step=".1" max="15" shortname="I_lat" />
        <dl_setting var="D_az_gain"  min="0" step=".0005" max=".01" shortname="D_lat" />
        <dl_setting var="P_el_gain" min="0" step=".1" max="15" shortname="P_long" />
        <dl_setting var="I_el_gain" min="0" step=".1" max="15" shortname="I_long" />
        <dl_setting var="D_el_gain"  min="0" step=".0005" max=".01" shortname="D_long" />
        <dl_setting var="Deadband_stick_yaw"  min="0" step="100" max="5000" shortname="Deadband" />
        <dl_setting var="Stick_gain_position"  min="0" step=".01" max="1" shortname="Stick_gain" />
      </dl_settings>
    </dl_settings>
  </settings>

  <header>
    <file name="Overactuated_mixing.h"/>
  </header>
  <init fun="overactuated_mixing_init()"/>
  <makefile>
    <file name="Overactuated_mixing.c"/>
  </makefile>
</module>

