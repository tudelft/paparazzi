<!DOCTYPE module SYSTEM "module.dtd">

<module name="Overactuated_mixing" dir="Overactuated_vehicle">
  <doc>
    <description>Overactuated Vehicle Mixing</description>
  </doc>


  <settings>
    <dl_settings>
      <dl_settings NAME="General overact">
        <dl_setting var="wind_speed"  min="0" step="1" max="20" shortname="wind_speed" />
        <dl_setting var="activate_tilting_az" min="0" step="1" max="1" shortname="tilt_az_active" values="FALSE|TRUE" />
        <dl_setting var="activate_tilting_el" min="0" step="1" max="1" shortname="tilt_el_active" values="FALSE|TRUE" />
        <dl_setting var="yaw_with_tilting" min="0" step="1" max="1" shortname="use_tilt_to_yaw" values="FALSE|TRUE" />
        <dl_setting var="yaw_with_motors" min="0" step="1" max="1" shortname="use_motor_to_yaw" values="FALSE|TRUE" />

        <dl_setting var="manual_roll_setpoint" min="0" step="1" max="1" shortname="man_roll_set" values="FALSE|TRUE" />
        <dl_setting var="manual_roll_overactuated_module"  min="-9600" step="960" max="9600" shortname="roll_setpoint" />

        <dl_setting var="manual_pitch_setpoint" min="0" step="1" max="1" shortname="man_pitch_set" values="FALSE|TRUE" />
        <dl_setting var="manual_pitch_overactuated_module"  min="-9600" step="960" max="9600" shortname="pitch_setpoint" />

        <dl_setting var="deadband_stick_yaw"  min="0" step="100" max="5000" shortname="Deadband yaw" />
        <dl_setting var="deadband_stick_throttle"  min="0" step="100" max="5000" shortname="Deadband throttle" />
        <dl_setting var="stick_gain_yaw"  min="0" step=".01" max="1" shortname="Stick gain yaw" />
        <dl_setting var="stick_gain_throttle"  min="0" step=".01" max="1" shortname="Stick gain thr" />


      </dl_settings>
    </dl_settings>
  </settings>

  <settings>
    <dl_settings>
      <dl_settings NAME="PID overact">

        <dl_setting var="pid_gains_over.p.phi" min="0" step="1" max="700" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_P_GAIN_PHI" shortname="PID_kp_roll" persistent="true"/>
        <dl_setting var="pid_gains_over.i.phi" min="0" step=".1" max="5" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_I_GAIN_PHI" shortname="PID_ki_roll" persistent="true"/>
        <dl_setting var="pid_gains_over.d.phi" min="0" step="1" max="45" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_D_GAIN_PHI" shortname="PID_kd_roll" persistent="true"/>

        <dl_setting var="pid_gains_over.p.theta" min="0" step="1" max="500"  module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_P_GAIN_THETA" shortname="PID_kp_pitch" persistent="true"/>
        <dl_setting var="pid_gains_over.i.theta" min="0" step=".1" max="5" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_I_GAIN_THETA" shortname="PID_ki_pitch" persistent="true"/>
        <dl_setting var="pid_gains_over.d.theta" min="0" step="1" max="100" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_D_GAIN_THETA" shortname="PID_kd_pitch" persistent="true"/>

        <dl_setting var="pid_gains_over.p.psi" min="0" step=".1" max="100" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_P_GAIN_PSI" shortname="PID_kp_yaw" persistent="true"/>
        <dl_setting var="pid_gains_over.i.psi" min="0" step=".1" max="5" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_I_GAIN_PSI" shortname="PID_ki_yaw" persistent="true"/>
        <dl_setting var="pid_gains_over.d.psi" min="0" step=".1" max="100" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_D_GAIN_PSI" shortname="PID_kd_yaw" persistent="true"/>

        <dl_setting var="pid_gains_over.p.x" min="0" step=".1" max="5" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_P_GAIN_POS_X" shortname="PID_kp_x" persistent="true"/>
        <dl_setting var="pid_gains_over.i.x" min="0" step=".1" max="5" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_I_GAIN_POS_X" shortname="PID_ki_x" persistent="true"/>
        <dl_setting var="pid_gains_over.d.x" min="0" step=".1" max="5" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_D_GAIN_POS_X" shortname="PID_kd_x" persistent="true"/>

        <dl_setting var="pid_gains_over.p.y" min="0" step=".1" max="5" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_P_GAIN_POS_Y" shortname="PID_kp_y" persistent="true"/>
        <dl_setting var="pid_gains_over.i.y" min="0" step=".1" max="5" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_I_GAIN_POS_Y" shortname="PID_ki_y" persistent="true"/>
        <dl_setting var="pid_gains_over.d.y" min="0" step=".1" max="5" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_D_GAIN_POS_Y" shortname="PID_kd_y" persistent="true"/>

        <dl_setting var="pid_gains_over.p.z" min="0" step=".1" max="500" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_P_GAIN_POS_Z" shortname="PID_kp_z" persistent="true"/>
        <dl_setting var="pid_gains_over.i.z" min="0" step=".1" max="500" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_I_GAIN_POS_Z" shortname="PID_ki_z" persistent="true"/>
        <dl_setting var="pid_gains_over.d.z" min="0" step="10" max="1000" module="Overactuated_vehicle/Overactuated_mixing" param="OVERACTUATED_MIXING_PID_D_GAIN_POS_Z" shortname="PID_kd_z" persistent="true"/>

      </dl_settings>
    </dl_settings>
  </settings>

  <settings>
    <dl_settings>
      <dl_settings NAME="INDI overact">
        <dl_setting var="indi_gains_over.p.phi" min="0" step=".001" max="5" shortname="INDI_kp_roll" param="OVERACTUATED_MIXING_PID_P_GAIN_PHI" persistent="true"/>
        <dl_setting var="indi_gains_over.d.phi" min="0" step=".001" max="5" shortname="INDI_kd_roll" param="OVERACTUATED_MIXING_PID_D_GAIN_PHI" persistent="true"/>

        <dl_setting var="indi_gains_over.p.theta" min="0" step=".001" max="5" shortname="INDI_kp_pitch" param="OVERACTUATED_MIXING_PID_P_GAIN_THETA" persistent="true"/>
        <dl_setting var="indi_gains_over.d.theta" min="0" step=".001" max="5" shortname="INDI_kd_pitch" param="OVERACTUATED_MIXING_PID_D_GAIN_THETA" persistent="true"/>

        <dl_setting var="indi_gains_over.p.psi" min="0" step=".001" max="5" shortname="INDI_kp_yaw" param="OVERACTUATED_MIXING_PID_P_GAIN_PSI_AZ" persistent="true"/>
        <dl_setting var="indi_gains_over.d.psi" min="0" step=".001" max="5" shortname="INDI_kd_yaw" param="OVERACTUATED_MIXING_PID_D_GAIN_PSI_AZ" persistent="true"/>

        <dl_setting var="indi_gains_over.p.x" min="0" step=".001" max="3" shortname="INDI_kp_x" param="OVERACTUATED_MIXING_PID_P_GAIN_POS_X" persistent="true"/>
        <dl_setting var="indi_gains_over.d.x" min="0" step=".001" max="3" shortname="INDI_kd_x" param="OVERACTUATED_MIXING_PID_D_GAIN_POS_X" persistent="true"/>

        <dl_setting var="indi_gains_over.p.y" min="0" step=".001" max="3" shortname="INDI_kp_y" param="OVERACTUATED_MIXING_PID_P_GAIN_POS_Y" persistent="true"/>
        <dl_setting var="indi_gains_over.d.y" min="0" step=".001" max="3" shortname="INDI_kd_y" param="OVERACTUATED_MIXING_PID_D_GAIN_POS_Y" persistent="true"/>

        <dl_setting var="indi_gains_over.p.z" min="0" step=".001" max="30" shortname="INDI_kp_z" param="OVERACTUATED_MIXING_PID_P_GAIN_POS_Z" persistent="true"/>
        <dl_setting var="indi_gains_over.d.z" min="0" step=".001" max="30" shortname="INDI_kd_z" param="OVERACTUATED_MIXING_PID_D_GAIN_POS_Z" persistent="true"/>

        <dl_setting var="act_dyn_struct.motor" min="0" step=".001" max=".5" shortname="act_dyn_thr" param="OVERACTUATED_MIXING_INDI_ACT_DYN_MOTOR" persistent="true"/>
        <dl_setting var="act_dyn_struct.elevation" min="0" step=".001" max=".5" shortname="act_dyn_el" param="OVERACTUATED_MIXING_INDI_ACT_DYN_EL" persistent="true"/>
        <dl_setting var="act_dyn_struct.azimuth" min="0" step=".001" max=".5" shortname="ect_dyn_az" param="OVERACTUATED_MIXING_INDI_ACT_DYN_AZ" persistent="true"/>

      </dl_settings>
    </dl_settings>
  </settings>


  <header>
    <file name="Overactuated_mixing.h"/>
  </header>
  <init fun="overactuated_mixing_init()"/>
  <makefile>
    <file name="Overactuated_mixing.c"/>
  </makefile>
</module>

